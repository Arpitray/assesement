{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "description": "React frontend (Create React App) for the App project",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "reactflow": "^11.8.3",
    "styled-components": "^6.1.19",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint \"src/**/*.{js,jsx}\" --max-warnings=0 || true",
    "format": "npx prettier --write \"src/**/*.{js,jsx,css,md,json}\"",
    "preview": "npx serve -s build",
    "clean": "npx rimraf build || (if (Test-Path build) { Remove-Item -Recurse -Force build })",
    "check": "npm run lint && npm test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
